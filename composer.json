{
  "name": "ngiraud/backend-blog",
  "description": "Add the availability to get a blog for the backend application",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Giraud",
      "email": "contact@ngiraud.me"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ngiraud/posttype-management": "~1.1",
    "illuminate/support": "~5.6.0"
  },
  "require-dev": {
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "NGiraud\\BackendBlog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NGiraud\\BackendBlog\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "NGiraud\\BackendBlog\\BackendBlogServiceProvider"
      ]
    }
  }
}
